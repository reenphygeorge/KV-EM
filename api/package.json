{
  "name": "api",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "test": "rm -rf dist/ && jest",
    "test:coverage": "jest --coverage",
    "build": "rm -rf dist/ && tsc",
    "start": "pnpm run build && node dist/src/app.js",
    "migration:create": "pnpm build && pnpm typeorm migration:create",
    "migration:generate": "pnpm build && pnpm typeorm migration:generate -d dist/src/db/data-source.db.js",
    "migration:run": "pnpm build && pnpm typeorm migration:run -d dist/src/db/data-source.db.js",
    "migration:revert": "pnpm build && pnpm typeorm migration:revert -d dist/src/db/data-source.db.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(t)s$": "ts-jest"
    },
    "verbose": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts",
      "!**/index.ts"
    ]
  }
}
